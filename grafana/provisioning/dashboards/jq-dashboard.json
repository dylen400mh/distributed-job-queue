{
  "__inputs": [],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" }
  ],
  "annotations": { "list": [] },
  "description": "Distributed Job Queue — operational dashboard covering jobs, workers, scheduler, and infrastructure.",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Jobs",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Number of jobs currently in each state, broken down by queue.",
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "short" },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "id": 1,
      "options": { "legend": { "calcs": ["lastNotNull"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (queue, status) (jq_job_queue_depth)",
          "legendFormat": "{{queue}} / {{status}}",
          "refId": "A"
        }
      ],
      "title": "Queue Depth by Status",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Rate of jobs transitioning to DONE, FAILED, or DEAD_LETTERED per second.",
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "reqps" },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "id": 2,
      "options": { "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (queue, status) (rate(jq_job_total{status=~\"DONE|FAILED|DEAD_LETTERED\"}[5m]))",
          "legendFormat": "{{queue}} / {{status}}",
          "refId": "A"
        }
      ],
      "title": "Job Completion Rate",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "End-to-end job processing duration percentiles per queue.",
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "s" },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "id": 3,
      "options": { "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (queue, le) (rate(jq_job_processing_duration_seconds_bucket[5m])))",
          "legendFormat": "p50 {{queue}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (queue, le) (rate(jq_job_processing_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 {{queue}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (queue, le) (rate(jq_job_processing_duration_seconds_bucket[5m])))",
          "legendFormat": "p99 {{queue}}",
          "refId": "C"
        }
      ],
      "title": "Job Processing Latency (p50 / p95 / p99)",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "gRPC request duration percentiles by RPC method.",
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "s" },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "id": 4,
      "options": { "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (method, le) (rate(jq_grpc_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 {{method}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (method, le) (rate(jq_grpc_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p99 {{method}}",
          "refId": "B"
        }
      ],
      "title": "gRPC Request Duration (p95 / p99)",
      "type": "timeseries"
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 },
      "id": 101,
      "title": "Workers",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Total number of jq-worker instances currently registered and online.",
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "green", "mode": "fixed" },
          "mappings": [{ "options": { "0": { "color": "red", "text": "0 — No workers!" } }, "type": "value" }],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": 0 }, { "color": "green", "value": 1 }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 18 },
      "id": 5,
      "options": { "colorMode": "background", "graphMode": "none", "justifyMode": "center", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "expr": "jq_worker_active_count",
          "legendFormat": "Workers Online",
          "refId": "A"
        }
      ],
      "title": "Workers Online",
      "type": "stat"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Active job count per worker relative to its concurrency limit.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "max": 8,
          "min": 0,
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 5 }, { "color": "red", "value": 8 }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 20, "x": 4, "y": 18 },
      "id": 6,
      "options": { "displayMode": "gradient", "orientation": "horizontal", "reduceOptions": { "calcs": ["lastNotNull"] }, "showUnfilled": true },
      "targets": [
        {
          "expr": "jq_worker_job_concurrency",
          "legendFormat": "{{worker_id}}",
          "refId": "A"
        }
      ],
      "title": "Active Jobs per Worker",
      "type": "bargauge"
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "id": 102,
      "title": "Scheduler",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Scheduler cycle duration. NFR-003 requires p95 < 200ms.",
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "s",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": 0 }, { "color": "yellow", "value": 0.1 }, { "color": "red", "value": 0.2 }] }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 23 },
      "id": 7,
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(jq_scheduler_cycle_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(jq_scheduler_cycle_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(jq_scheduler_cycle_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "title": "Scheduler Cycle Duration",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Rate of jobs assigned to workers per second, by queue.",
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "reqps" },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 23 },
      "id": 8,
      "options": { "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (queue) (rate(jq_scheduler_jobs_assigned_total[5m]))",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "title": "Jobs Assigned / sec",
      "type": "timeseries"
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 30 },
      "id": 103,
      "title": "Infrastructure",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "DB query duration percentiles by query name.",
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "s" },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 31 },
      "id": 9,
      "options": { "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (query_name, le) (rate(jq_db_query_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 {{query_name}}",
          "refId": "A"
        }
      ],
      "title": "DB Query Duration p95",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Redis operation duration percentiles by operation type.",
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "s" },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 31 },
      "id": 10,
      "options": { "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (operation, le) (rate(jq_redis_operation_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 {{operation}}",
          "refId": "A"
        }
      ],
      "title": "Redis Operation Duration p95",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Rate of Kafka publish failures by topic. Any non-zero value means audit events are being dropped.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "reqps",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": 0 }, { "color": "red", "value": 0.001 }] }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 31 },
      "id": 11,
      "options": { "legend": { "calcs": ["lastNotNull"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum by (topic) (rate(jq_kafka_publish_errors_total[5m]))",
          "legendFormat": "{{topic}}",
          "refId": "A"
        }
      ],
      "title": "Kafka Publish Error Rate",
      "type": "timeseries"
    }

  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["jq", "job-queue"],
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Distributed Job Queue",
  "uid": "jq-overview",
  "version": 1
}
